{
  "extends": [
    "next/core-web-vitals",
    "next/typescript"
  ],
  "rules": {
    // Disable the specific rules that are causing your errors
    "@typescript-eslint/no-explicit-any": "off", // For the "Unexpected any" errors
    "@typescript-eslint/no-unused-vars": "off", // For all unused variables errors
    "prefer-const": "off", // For "never reassigned. Use 'const'" errors
    "react/no-children-prop": "off", // For children prop errors
    "@typescript-eslint/no-empty-object-type": "off" // For empty interface error
  }
}